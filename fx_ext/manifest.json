{
    "manifest_version": 2,
    "name": "__MSG_extension_name__",
    "version": "1.0.6",
    "author": "JumuFENG",
    "description": "__MSG_extension_description__",
    "default_locale": "zh_CN",
    "browser_action": {
        "default_title": "__MSG_popuptitle__",
        "default_icon": {
            "19": "icons/fhu_19.png",
            "38": "icons/fhu_38.png"
        },
        "default_popup": "ui/popup/index.html"
        //"browser_style": true
    },
    "icons": {
        "16": "icons/fhu_16.png",
        "48": "icons/fhu_48.png",
        "128": "icons/fhu_128.png"
    },
    "background": {
        "page": "background/index.html"
    },
    "content_scripts": [
        {
            "matches": [
                "file:///*summary.html",
                "http://47.100.77.253/*summary*"
            ],
            "js": [
                "inject/index.js"
            ],
            "run_at": "document_idle",
            "all_frames": true,
            "match_about_blank": false
        },
        {
            "matches": [
                "https://jywg.18.cn/*"
            ],
            "js": [
                "inject/jywg_conditions.js"
            ],
            "run_at": "document_idle",
            "all_frames": true,
            "match_about_blank": false
        },
        {
            "matches": [
                "http://www.iwencai.com/*"
            ],
            "js": [
                "inject/wencai_inject.js"
            ],
            "run_at": "document_idle",
            "all_frames": true,
            "match_about_blank": false
        }
    ],
    "permissions": [
        "storage",
        "tabs",
        "cookies",
        "alarms",
        "downloads",
        "<all_urls>"
    ],
    "commands": {
    },
    "applications": {
        "gecko": {
            "id": "fundholdupdate@jumufeng.me",
            "strict_min_version": "57.0"
        }
    }
}
